.myConsumption_title{
    background-color: #e6e6e6;
    color: #03bf01;
    font-size: 30rpx;
    justify-content: center;
    align-items: center;
    height: 60rpx;
    line-height: 60rpx;
    padding: 10rpx 0;
    border-bottom: 1px solid #dbdbdb;
    border-top: 1px  dashed #dbdbdb;
}
.myConsumption_title .title{
    width: 25%;
    text-align: center;
    display: block;
    float: left;
    border-right: 1px solid #fff;
    box-sizing: border-box;
}
.myConsumption_title .title:nth-last-child(1){
    border-right: 0;
}
.myConsumption_item_row{
    border-bottom: 1px solid #dbdbdb;
    font-size: 30rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding: 10rpx;
}
.myConsumption_item_row .title{
    width: 25%;
    text-align: center;
    display: block;
    float: left;
    box-sizing: border-box;
}
.myConsumption_item_row .title_2{
    width: 50%;
    text-align: left;
}
.text_right{
    text-align:right;
}
.go_font {
    display: block;
}

.go_font:before {
    content: " ";
    display: inline-block;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    height: 0.45rem;
    width: 0.45rem;
    top: 0;
    border-width: 0 0 0.08rem 0.08rem;
    border-color: #555;
    border-style: solid;
    position: relative;
    -webkit-transform: rotate(225deg);
    transform: rotate(225deg);
}
.go_font.up:before {
    border-width: .08rem 0 0 .08rem;
}

.hidden{
    display: none;
}
.show{
    display: block;
}
